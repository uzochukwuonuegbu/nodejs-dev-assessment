{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "../",
  "testRegex": ".spec.js$",
  "transform": {
    "\\.js$": "<rootDir>/node_modules/babel-jest"
  },
  "coverageDirectory": "../coverage",
  "testEnvironment": "node",
  "testTimeout": 20000,
  "coveragePathIgnorePatterns": [
  ],
  "setupFilesAfterEnv": [
    "<rootDir>/test/jest.setup.js"
  ]
}
